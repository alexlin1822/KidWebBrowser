{
  "expo": {
    "scheme": "acme",
    "icon": "./app/assets/icon.png",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./app/assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "name": "KidWebBrowser",
    "slug": "KidWebBrowser",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "b91e20bc-9aa8-483c-b56e-eb16e350a2ee"
      }
    },
    "android": {
      "package": "com.alexlam1822.KidWebBrowser",
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.alexlam1822.KidWebBrowser"
     }
  }
}
